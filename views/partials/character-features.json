{
    "views": [
      {
        "type": "list",
        "attribute": "data.classes",
        "views": [
          {
            "style": "heading2",
            "value": "{{name}} Features",
            "padding": [4, 0]
          },
          {
            "type": "list",
            "attribute": "features",
            "divider": true,
            "views": [
              {
                "type": "disclosureGroup",
                "title": "{{name}}",
                "value": "Level {{level}}",
                "style": "heading4",
                "views": [
                  {
                    "type": "text",
                    "value": "{{text}}"
                  }
                ]
              },
            ]
          },
        ]
      },
      {
        "style": "heading2",
        "value": "{{data.race.name}} Traits",
        "padding": [4, 0]
      },
      {
        "type": "list",
        "attribute": "data.race.traits",
        "views": [
          {
            "type": "text",
            "value": "**{{name}}**. {{text}}",
          },
        ]
      },
      {
        "type": "list",
        "attribute": "data.feats",
        "views": [
          {
            "style": "heading2",
            "value": "Feat: {{name}}",
            "padding": [4, 0],
            "link": "{{reference}}",
          },
          {
            "type": "text",
            "value": "{{descr}}",
          },
        ]
      }
    ]
  }